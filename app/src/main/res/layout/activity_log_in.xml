        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="393dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="73dp"
        android:layout_height="34dp"
        android:text="LogIn"
